fun fibonacciIterative(n) {
    if (n == 0) {
        return 0;  // Caso base: Fibonacci(0) = 0
    }
    if (n == 1) {
        return 1;  // Caso base: Fibonacci(1) = 1
    }

    let a = 0;  // Fibonacci(0)
    let b = 1;  // Fibonacci(1)
    let fib = 0;

    let i = 2;
    while (i <= n) {
        fib = a + b;  // Calcula el siguiente número de Fibonacci
        a = b;        // Actualiza a con el valor de b
        b = fib;      // Actualiza b con el nuevo Fibonacci
        i = i + 1;
    }

    return fib;
}

let numero = 7;
print("Calculando el n-ésimo número de Fibonacci de manera iterativa:");
let resultado = fibonacciIterative(numero);
print("El Fibonacci de " + numero + " es: " + resultado);
