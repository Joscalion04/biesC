fun factorialIterative(n) {
    let result = 1;

    let i = 1;
    while (i <= n) {
        result = result * i;
        i = i + 1;
    }

    return result;
}

let numero = 5;
print("Calculando el factorial de un nÃºmero de manera iterativa:");
let resultado = factorialIterative(numero);
print("El factorial de " + numero + " es: " + resultado);
