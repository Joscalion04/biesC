fun isPalindromeIterative(lista) {
    let i = 0;
    let j = len(lista) - 1;

    while (i < j) {
        if (lista[i] != lista[j]) {
            return false;  // No es un palíndromo
        }
        i = i + 1;
        j = j - 1;
    }

    return true;  // Es un palíndromo
}

let lista = [1, 2, 3, 2, 1];
print("Verificando si la lista es un palíndromo de manera iterativa:");
let resultado = isPalindromeIterative(lista);
if (resultado) {
    print("La lista es un palíndromo.");
} else {
    print("La lista no es un palíndromo.");
}
